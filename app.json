{
  "expo": {
    "name": "Auto2048",
    "slug": "auto2048",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "platforms": ["ios", "android", "web"],
    "experiments": {
      "baseUrl": "/auto2048"
    },
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FAF8EF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FAF8EF"
      }
    },
    "web": {
      "bundler": "metro"
    }
  }
} 